<!--begin::Page Snippets -->
<script src="/js/dashboard.js" type="text/javascript"></script>
<!--end::Page Snippets -->

<script src="/js/user/feed.js" type="text/javascript"></script>
<script src="/js/common/common.js" type="text/javascript"></script>

</body>
<!-- end::Body -->
</html
